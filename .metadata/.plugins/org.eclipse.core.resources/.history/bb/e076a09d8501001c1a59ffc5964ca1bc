package Condinalstatement;
import java.util.Scanner;
import java.util.Scanner;
public class Greastnumber {

	public static void main(String[] args) {
		int a,b;
		Scanner sc = new Scanner(System.in);
		a=sc.nextInt();
		b=sc.nextInt();
		{
			System.out.print("enter the value");
		}
		if(a==b)
		{
			System.out.print("a and b vlues equal");
			
		}
		 else if (a>b)
		{
			System.out.print("a is greater then b");
		}
		else
		{
			System.out.print("b is grater then a");
		}
		}
	}


