package com.demo;
import java.util.Scanner;
public class HariHara {

	public static void main(String[] args) {
		int ch;
		@SuppressWarnings("resource")
		Scanner read = new Scanner(System.in);
		ch=read.nextInt();
		char hari=(char)ch;
		System.out.print(hari);
		
		
		// TODO Auto-generated method stub

	}

}
